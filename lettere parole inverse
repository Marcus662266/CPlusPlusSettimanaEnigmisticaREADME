#include <iostream>
#include <string>

using namespace std;

int main() {
    string input_string;
    cout << "Inserisci una stringa di caratteri: ";
    getline(cin, input_string);

    cout << "La stringa al contrario e': ";
    for (int i = input_string.length() - 1; i >= 0; i--) {
        cout << input_string[i];
    }
    cout << endl;

    return 0;
}
